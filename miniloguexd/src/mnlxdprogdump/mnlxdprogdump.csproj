    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>MinilogueXD.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="MinilogueXD.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="userOscillatorDescriptions.json">
